{
	"name": "@mythrantic/svelte-leaflet",
	"version": "1.1.2",
	"publishConfig": {
		"access": "public"
	},
	"description": "Svelte leaflet components library used in mythrantic projects. easily add maps to your sveltekit project.",
	"maintainers": [
		"valiantlynx <valiantlynxz@gmail.com> (https://www.valiantlynx.com/)"
	],
	"keywords": [
		"sveltekit",
		"svelte",
		"leaflet",
		"typescript",
		"map",
		"components",
		"blocks"
	],
	"author": {
		"name": "valiantlynx",
		"email": "valiantlynxz@gmail.com"
	},
	"contributors": [
		{
			"name": "valiantlynx",
			"email": "valiantlynxz@gmail.com"
		}
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mythrantic/svelte-leaflet.git"
	},
	"homepage": "https://svelte-leaflet.valiantlynx.com",
	"bugs": {
		"url": "https://github.com/mythrantic/svelte-leaflet/issues"
	},
	"scripts": {
		"dev": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package",
		"package:check": "publint",
		"release": "yarn package && yarn package:check && yarn changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepublishOnly": "yarn package",
		"ci:publish": "pnpm publish -r --no-git-checks",
		"ci:version": "pnpm changeset version"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"README.md",
		"LICENSE"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"leaflet": "^1.9.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.15.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.0.0",
		"@valiantlynx/eslint-config-custom": "^0.0.1",
		"@valiantlynx/tailwind-config": "^0.0.9",
		"@valiantlynx/tsconfig": "^0.0.25",
		"autoprefixer": "^10.4.20",
		"dotenv": "^17.0.0",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"leaflet": "^1.9.4",
		"leaflet-routing-machine": "^3.2.12",
		"postcss": "^8.4.49",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"publint": "^0.3.12",
		"svelte": "^5.15.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.7",
		"tailwindcss": "^4.0.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^7.3.0"
	},
	"dependencies": {
		"leaflet.fullscreen": "^4.0.0"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
